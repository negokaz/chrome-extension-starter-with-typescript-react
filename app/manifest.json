{
    "manifest_version": 2,
    "name": "Chrome Extension Starter",
    "description": "A Chrome Extension starter kit with TypeScript and React",
    "version": "0.0.0",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "optional_permissions": [
      "http://*/",
      "https://*/"
    ],
    "background": {
      "scripts": [
        "scripts/background.js"
      ]
    },
    "browser_action": {
    },
    "options_page": "views/options.html"
  }
